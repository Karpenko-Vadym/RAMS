<!-- REPORT INDEX VIEW -->
<button type="button" class="btn btn-info pull-right" onclick="LoadAction('position-list-div', '@Url.Action("PositionList", "Report", new { Area = "Agency" })' );">Refresh List</button>

<div class="clearfix"></div>

<div class="vertical-padding-24"></div>

<div id="position-list-div"></div>

<!-- POSITION STATUS REPORT MODAL -->
<div id="position-status-report-modal" class="modal fade" role="dialog">
    <div class="modal-dialog modal-lg">
        <!-- MODAL CONTENT -->
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" onclick="$('#position-status-report-modal').modal('hide');">&times;</button>
                <h4 class="modal-title text-center">Position Status Report</h4>
            </div>
            <div class="modal-body">
                <div class="well well-lg bottom-margin-0">
                    <div id="position-status-report-modal-body-div"></div>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" onclick="$('#position-status-report-modal').modal('hide');">Close</button>
            </div>
        </div>
        <!-- END OF MODAL CONTENT-->
    </div>
</div>
<!-- END OF POSITION STATUS REPORT MODAL -->

<!-- POSITION FINAL REPORT MODAL -->
<div id="position-final-report-modal" class="modal fade" role="dialog">
    <div class="modal-dialog modal-lg">
        <!-- MODAL CONTENT -->
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" onclick="$('#position-final-report-modal').modal('hide');">&times;</button>
                <h4 class="modal-title text-center">Position Final Report</h4>
            </div>
            <div class="modal-body">
                <div class="well well-lg bottom-margin-0">
                    <div id="position-final-report-modal-body-div"></div>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" onclick="$('#position-final-report-modal').modal('hide');">Close</button>
            </div>
        </div>
        <!-- END OF MODAL CONTENT-->
    </div>
</div>
<!-- END OF POSITION FINAL REPORT MODAL -->

<script type="text/javascript">$(function () { LoadAction("position-list-div", "@Url.Action("PositionList", "Report", new { Area = "Agency" })"); AgencyModalControls(); });</script>
<!-- END OF REPORT INDEX VIEW -->